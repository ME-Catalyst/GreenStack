	<xsd:complexType name="RecordT">
		<xsd:complexContent>
			<xsd:extension base="ComplexDatatypeT">
				<xsd:sequence maxOccurs="unbounded">
					<xsd:element name="RecordItem" type="RecordItemT"/>
				</xsd:sequence>
				<xsd:attribute name="bitLength" use="required">
					<xsd:simpleType>
						<xsd:union>
					<xsd:simpleType>
						<xsd:restriction base="BitCountT">
							<xsd:minInclusive value="1"/>
							<xsd:maxInclusive value="1856"/>
						</xsd:restriction>
					</xsd:simpleType>
							<xsd:simpleType>
								<xsd:restriction base="TbdValueListT"/>
							</xsd:simpleType>
						</xsd:union>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RecordItemT">
		<xsd:sequence>
			<xsd:choice maxOccurs="2">
				<xsd:element name="SimpleDatatype" type="SimpleDatatypeT">
					<!-- <xsd:unique name="RecordItem_SimpleDatatype_Value">
						<xsd:selector xpath="iodd:SingleValue|iodd:ValueRange"/>
						<xsd:field xpath="@value|@lowerValue"/>
					</xsd:unique> -->
				</xsd:element>
				<xsd:element name="DatatypeRef" type="DatatypeRefT"/>
			</xsd:choice>
			<xsd:element name="Name" type="TextRefT"/>
			<xsd:element name="Description" type="TextRefT" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="subindex" use="required">
			<xsd:simpleType>
				<xsd:union>
					<xsd:simpleType>
						<xsd:restriction base="SubindexT"/>
					</xsd:simpleType>
					<xsd:simpleType>
						<xsd:restriction base="TbdValueListT"/>
					</xsd:simpleType>
				</xsd:union>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="bitOffset" use="required">
			<xsd:simpleType>
				<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="BitCountT">
					<xsd:maxInclusive value="1855"/>
				</xsd:restriction>
			</xsd:simpleType>
					<xsd:simpleType>
						<xsd:restriction base="TbdValueListT"/>
					</xsd:simpleType>
				</xsd:union>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="accessRightRestriction" type="AccessRightsT"/>
		<xsd:attribute name="checkElement" type="ElementChecksT"/>
    <xsd:attribute name="profileConstraints" type="ProfileConstraintsT"/>
  </xsd:complexType>

	<xsd:complexType name="NumberT" abstract="true">
		<xsd:complexContent>
			<xsd:extension base="SimpleDatatypeT"/>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="IntegerT">
		<xsd:complexContent>
			<xsd:extension base="NumberT">
				<xsd:sequence>
